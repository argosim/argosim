package at.ac.univie.knasmueller.argos.explorer;
import java.io.File;
import java.io.IOException;

import at.cibiv.codoc.CoverageDecompressor;
import at.cibiv.codoc.utils.CodocException;
import at.cibiv.ngs.tools.util.GenomicPosition;

public class CodocTest {

	public static void main(String[] args) throws CodocException, IOException {
		CoverageDecompressor d = null;
		try {

			d = CoverageDecompressor
					.loadFromFile(
							new File(
									"/project/bakk/ARGOS/out_niko/eck12_MG1655_ecoli-chr-GENOME.ISS.wig.codoc"),
							null);
			for (int i = 0; i < 10; i++) {
				System.out.println(d.query(new GenomicPosition("chr", i + 2286937)));
			}
			
		} finally {
			d.close();
		}

	}
}
